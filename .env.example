# Pulse WiFi Environment Variables

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# GWN Cloud API
GWN_APP_ID=your-app-id
GWN_SECRET_KEY=your-secret-key
GWN_API_BASE=https://www.gwn.cloud/api

# Mapbox
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token

# Passpoint Configuration
PASSPOINT_REALM=pulsewifi.co.uk
PASSPOINT_OPERATOR_FQDN=pulsewifi.co.uk
PASSPOINT_RADIUS_HOST=radius.pulsewifi.co.uk
PASSPOINT_RADIUS_SECRET=your-radius-secret
PASSPOINT_EAP_METHOD=TTLS
PASSPOINT_INNER_AUTH=MSCHAPv2
PASSPOINT_HS_FRIENDLY_NAME=Pulse WiFi
PASSPOINT_HS_VENUE_NAME=Pulse WiFi Venue
PASSPOINT_HS_NAI_REALM=pulsewifi.co.uk
PASSPOINT_SSID=Pulse-WiFi
PASSPOINT_CA_CERT_URL=
PASSPOINT_ORG_ID=uk.co.pulsewifi

# App
NEXT_PUBLIC_APP_URL=https://app.pulsewifi.co.uk

# OneSignal Push Notifications (Web Push)
NEXT_PUBLIC_ONESIGNAL_APP_ID=your-onesignal-app-id
NEXT_PUBLIC_ONESIGNAL_SAFARI_WEB_ID=web.onesignal.auto.xxxxxx
ONESIGNAL_APP_ID=your-onesignal-app-id
ONESIGNAL_API_KEY=your-onesignal-rest-api-key

# Internal API Keys
INTERNAL_API_KEY=generate-a-secure-random-key
CRON_SECRET=generate-another-secure-random-key
